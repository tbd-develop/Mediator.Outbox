    <ItemGroup>
      <ProjectReference Include="..\TbdDevelop.Mediator.Outbox\TbdDevelop.Mediator.Outbox.csproj" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.EntityFrameworkCore" Version="8.0.0" />
