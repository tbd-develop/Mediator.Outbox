<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Version>0.0.1</Version>
        <Title>Mediator Outbox SqlServer</Title>
        <Authors>Terry Burns-Dyson</Authors>
        <Description>SqlServer implementation of Outbox</Description>
        <PackageProjectUrl>https://github.com/tbd-develop/mediator.outbox</PackageProjectUrl>
        <RepositoryUrl>https://github.com/tbd-develop/mediator.outbox</RepositoryUrl>
